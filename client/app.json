{
  "expo": {
    "name": "client",
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "expo": {
      "plugins": [
        [
          "expo-font",
          {
            "fonts": [
              "./assets/fonts/e-Ukraine-Bold.otf", 
              "./assets/fonts/e-Ukraine-Light.otf", 
              "./assets/fonts/e-Ukraine-Medium.otf",
              "./assets/fonts/e-Ukraine-Regular.otf"
            ]
          }
        ]
      ]
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
